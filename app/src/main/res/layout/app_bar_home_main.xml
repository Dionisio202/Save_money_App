<?xml version="1.0" encoding="utf-8"?>
<androidx.coordinatorlayout.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Home_main">

    <com.google.android.material.appbar.AppBarLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:theme="@style/Theme.Save_Money.AppBarOverlay"
        app:titleTextColor="@color/nav_bar_color"
        app:subtitleTextColor="@color/nav_bar_color"
        app:iconTint="@color/nav_bar_color"
        >

        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbar"
            android:layout_width="match_parent"
            android:layout_height="?attr/actionBarSize"
            android:background="@color/nav_bar_header_color"
            app:popupTheme="@style/Theme.Save_Money.PopupOverlay"

            />

    </com.google.android.material.appbar.AppBarLayout>

    <include layout="@layout/content_home_main" />



</androidx.coordinatorlayout.widget.CoordinatorLayout>